<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:d="http://xamarin.com/schemas/2014/forms/design"
            xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
            xmlns:views="clr-namespace:MaxVonGrafKftMobile.Views" 
            xmlns:local="clr-namespace:MaxVonGrafKftMobile.MarkupExtensions"
            mc:Ignorable="d"
            x:Class="MaxVonGrafKftMobile.Views.HomeTabbedPage"
            xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"
            ios:Page.UseSafeArea="True"
            BackgroundColor="Transparent"
            BarBackgroundColor="Transparent"
            NavigationPage.HasNavigationBar="False"
           >
    <!--Pages can be added as references or inline-->
    <views:HomePageDetail
        x:Name="Dashboard"
        Title="Home"
        IconImageSource="iconRedDashboard.png"
        HeightRequest="30"/>
    <views:VehicleDetailPage
        x:Name="bookingPage"
        Title="Book Now"
        IconImageSource="iconRedCar.png"/>
    <views:MyDouments
        x:Name="MembershipScreen"
        Title="Documents"
        IconImageSource="IconeRedFolder.png"/>
    <views:ChatListPage
        x:Name="AcountPage"
        Title="Chat"
        IconImageSource="IconRedchat.png"/>

  
</TabbedPage>